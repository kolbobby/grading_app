<% provide(:title, @user.name) %>
<div class="col pull-left tabular">
	<table>
		<thead>
			<tr>
				<td></td>
				<td><h3><%= @user.name %></h3></td>
			</tr>
		</thead>
		<% if @user.teacher? %>
			<%= render 'teacher_side' %>
		<% else %>
			<%= render 'student_side' %>
		<% end %>
	</table>
</div>
<div id="main-user-content" class="tabular">
	<div class="row">
		<% if @user.teacher? %>
			This is the home portion of the teacher profile page.<br />
		<% else %>
			This is the home portion of the student profile page.
		<% end %>
	</div>
</div>