<% provide(:title, @user.name) %>
<% if current_user == User.find(params[:id]) %>
	<div class="col pull-left">
		<%= render 'teacher_side' %>
	</div>
	<div id="main-user-content" class="pull-right">
		This is the home portion of the teacher profile page.
	</div>
<% else %>
	<div id="main-user-content" class="pull-right" style="width:100%;">
		This is the home portion of the teacher profile page.
	</div>
<% end %>